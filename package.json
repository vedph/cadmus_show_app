    "build-shop": "ng build @myrmidon/cadmus-shop-asset --configuration production && ng build @myrmidon/cadmus-shop-ui --configuration production",
    "build-lib": "npm run build-core && npm run build-ui && npm run build-prof && npm run build-shop",
    "build-all": "npm run build-core && ng build @myrmidon/cadmus-shop-asset --configuration production && ng build @myrmidon/cadmus-show-ui --configuration production && ng build @myrmidon/cadmus-profile-ui --configuration production && ng build @myrmidon/cadmus-shop-ui --configuration production"
  },
