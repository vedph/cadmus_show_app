    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-core": "ng build @myrmidon/cadmus-shop-core --configuration production && ng build @myrmidon/cadmus-profile-core --configuration production",
    "build-ui": "ng build @myrmidon/cadmus-show-ui --configuration production && ng build @myrmidon/cadmus-show-viz --configuration production",
